{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "test/**/*.ts",
    "test/**/*.tsx",
    "src/commands/**/*.ts"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "reporter": [
    "text",
    "html",
    "json"
  ],
  "sourceMap": true,
  "instrument": true,
  "report-dir": "./coverage/Playwright"
}
